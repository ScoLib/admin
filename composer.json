{
    "name": "scolib/admin",
    "type": "library",
    "description": "laravel admin",
    "keywords": [
        "ScoLib",
        "admin"
    ],
    "homepage": "https://github.com/ScoLib/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "klgd",
            "email": "slice1213@gmail.com",
            "homepage": "http://www.scophp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "illuminate/support": "~5.4",
        "league/fractal": "~0.15.0",
        "scolib/tree": "~1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Sco\\Admin\\": "src"
        },
        "classmap": [
            "database/seeds/AdminTableSeeder.php",
            "database/seeds/RbacTableSeeder.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sco\\Admin\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "php-cs-fixer fix ./"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
