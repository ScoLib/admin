{
    "title": "Sco Admin",
    "description": "Sco Admin",
    "author": "klgd",
    "output.name": "site",
    "gitbook": ">3.0.0",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "-highlight",
        "-livereload",
        "search-plus",
        "simple-page-toc",
        "github",
        "github-buttons",
        "edit-link",
        "-disqus"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "disqus": {
            "shortName": "scophpdocs"
        },
        "github": {
            "url": "https://github.com/scolib/admin"
        },
        "github-buttons": {
            "repo": "scolib/admin",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "edit-link": {
            "base": "https://github.com/scolib/admin/edit/master/docs",
            "label": "Edit This Page"
        }
    }
}
