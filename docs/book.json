{
    "title": "Sco Admin",
    "description": "Sco Admin",
    "author": "klgd",
    "output.name": "site",
    "gitbook": ">3.0.0",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "-highlight",
        "-livereload",
        "-disqus",
        "search-plus",
        "github",
        "github-buttons",
        "edit-link",
        "expandable-chapters",
        "tbfed-pagefooter",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "disqus": {
            "shortName": "scophpdocs"
        },
        "github": {
            "url": "https://github.com/ScoLib/admin"
        },
        "github-buttons": {
            "buttons": [
                {
                    "user": "ScoLib",
                    "repo": "admin",
                    "type": "star",
                    "size": "small",
                    "count": true
                }
            ]
        },
        "edit-link": {
            "base": "https://github.com/scolib/admin/edit/master/docs",
            "label": "Edit This Page"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy <a href=\"http://www.scophp.com\" target=\"_blank\">scophp.com</a> 2016-2017",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        }
    }
}
